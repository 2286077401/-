
.foot-nav {
  width:100%;
  height:110rpx;
  position: fixed;
  bottom: 0;
  z-index: 999;
  border-top:2rpx solid #f4f4f4;
  box-sizing:border-box;
  color:#999;
  background:#fff;
  /* box-shadow:0px -5rpx 10rpx rgba(209, 209, 209, 0.3); */
}
.foot-nav .tab-bar-item {
  position:relative;
  font-size:25rpx;
  width:20%;
  height:110rpx;
  display: inline-block;
  text-align: center;
  top: -20rpx;

}
.foot-nav .tab-bar-item .img-box {
  top: -8rpx;
  position: relative;
}
.foot-nav .tab-bar-item:nth-child(3) .img-box {
  top: -20rpx;
  position: relative;
}
.foot-nav .tab-bar-item image {
  display: block;
  width:50rpx;
  height:50rpx;
  margin:0 auto;
  margin-top: -10rpx;
}
.foot-nav navigator:nth-child(3){
  position: relative;
  top:-40rpx;
}
.foot-nav .tab-bar-item:nth-child(3) image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background: white;
  padding: 10rpx;
  
}