/* pages/crm/tel/tel.wxss */
page {
  height: 100%;
  background:#FFFFFF ;
}

.all {
  width: 100%;
  position: relative;
 
}
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 60rpx;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.7;
}

.top {
  background-color: white;
  display: flex;
  position: fixed;
  width: 680rpx;
  right: 50%;
  margin-right: -350rpx;
  border: 1rpx solid #D5DFE0;
  border-radius: 10rpx;
  padding: 10rpx;
  line-height: 10rpx;
}

.center {
  width: 680rpx;
  padding: 10rpx;
  margin: 0 auto;
  display: flex;
  padding-top: 50rpx;
}

.center:nth-child(2) {
  padding-top: 100rpx;
}

.topinput {
  margin-left: 20rpx;
}

.serchImg {
  width: 50rpx;
  height: 50rpx
}

.phone {
  width: 50rpx;
  height: 50rpx;
}

.head {
  width: 50rpx;
  height: 50rpx;
  display: block;
  border-radius: 50rpx;
}

.top {
  z-index: 100;
}
.tanchu{
  background-color: white;
  position: absolute;
  top: 160rpx;
z-index: 9010;
  height: 800rpx;
  width: 600rpx;
  left: 50%;
  margin-left: -300rpx;
  text-align: center;
  line-height: 100rpx;

}