/* packageB/pages/Supplier/packagingaudit.wxss */
page {
  background-color: #F1F1F0;
  padding-bottom: 120rpx
}

.active {
  background: #D5941E;
  color: white;
}

.none {
  background: none;
  color: black;
}

.box {
  display: flex;

}
 

.rui-justify {
  text-align: justify;
  position: relative;
}

input {
  background: white;
  border-radius: 10rpx;
  margin: 0 10rpx 0 10rpx;
}

.rui-justify::after {
  content: '';
  display: inline-block;
  padding-left: 100%;
}