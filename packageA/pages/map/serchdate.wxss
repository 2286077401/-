/* 模态框 */
/*mask*/
.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}
.drawer_box {
  width: 450rpx;
  max-height: 800rpx;
  position: fixed;
  top: 35%;
  left: 0;
  z-index: 1001;
  background: #FFFFFF;
  margin: -150px 145rpx 0 145rpx;
  border-radius: 3px;
  overflow: scroll;
  overflow-y: scroll;
}

.tccontent {
  display: flex;
  height: 60rpx;
  width: 390rpx;

}

.tctop {
  flex: 8;
  height: 60rpx;
  overflow: hidden;
  border-bottom: 1rpx solid gray;
}

.serchbtnbox {
  flex: 2;
  height: 60rpx;
  border-radius: 10rpx;
}

.drawer_content {
  padding: 20rpx;
  overflow: scroll;
  overflow-y: scroll;
  max-height: 600rpx;
  width: 410rpx;

}

.weui-uploader__input-box {
  border: dashed #CCCCCC !important;
  background-color: white;
}